{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    },
    "lib": ["es5", "es6", "dom", "dom.iterable"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "paths": {
    "crypto": [
      "node_modules/crypto-js"
    ]
  },
}
